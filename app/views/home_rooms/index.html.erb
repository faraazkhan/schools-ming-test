<h1>Listing home_rooms</h1>

<table>
  <tr>
    <th>Teacher</th>
    <th>Homeroom number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @home_rooms.each do |home_room| %>
  <tr>
    <td><%= home_room.teacher_id %></td>
    <td><%= home_room.homeroom_number %></td>
    <td><%= link_to 'Show', home_room %></td>
    <td><%= link_to 'Edit', edit_home_room_path(home_room) %></td>
    <td><%= link_to 'Destroy', home_room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

